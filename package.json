    "@ory/integrations": "0.2.8",
    "@ory/themes": "~0.0.101",
    "axios": "^1.2.6",
    "classnames": "^2.3.1",
    "csurf": "^1.11.0",
    "express": "^4.18.2",
    "hydra-js": "^0.1.5",
    "next": "12.1.6",
    "pkce-challenge": "^3.0.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-toastify": "^8.0.3",
