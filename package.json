{
  "name": "@ory/integration-react",
  "version": "0.0.1",
  "config": {
    "prettierTarget": "**/*.{tsx,ts,json,md,js,css}"
  },
  "scripts": {
    "dev": "next dev",
    "build:server": "tsc /server /server/dist",
    "dev:server": "nodemon server/index.js",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write ${npm_package_config_prettierTarget}",
