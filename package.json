    "@types/express": "^4.17.15",
    "@types/node": "^18.11.18",
    "@types/react": "~17.0.19",
    "@types/request": "^2.48.7",
    "@types/styled-components": "^5.1.13",
    "concurrently": "^7.6.0",
    "cypress": "^8.7.0",
    "eslint": "7.32.0",
    "eslint-config-next": "12.0.3",
    "nodemon": "^2.0.20",
    "ory-prettier-styles": "^1.1.2",
    "prettier": "^2.3.2",
    "rollup": "^2.56.3",
    "rollup-plugin-dts": "^4.0.0",
    "rollup-plugin-esbuild": "^4.5.0",
